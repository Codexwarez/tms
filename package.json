{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "dev:win": "concurrently -c \"#93c5fd,#c4b5fd,#fb7185\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"vite\" --names=server,queue,vite --kill-others",
        "dev:unix": "concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"vite\" --names=server,queue,logs,vite --kill-others",
        "dev": "node -e \"require('child_process').execSync(process.platform === 'win32' ? 'npm run dev:win' : 'npm run dev:unix', {stdio: 'inherit'})\""
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.2",
        "@tailwindcss/vite": "^4.1.12",
        "alpinejs": "^3.4.2",
        "autoprefixer": "^10.4.2",
        "axios": "^1.11.0",
        "concurrently": "^9.2.1",
        "laravel-vite-plugin": "^2.0.0",
        "postcss": "^8.4.31",
        "tailwindcss": "^3.4.17",
        "vite": "^7.0.4"
    },
    "dependencies": {
        "apexcharts": "^3.46.0",
        "flowbite": "^3.1.2",
        "laravel-echo": "^2.2.0",
        "pusher-js": "^8.4.0",
        "swiper": "^11.2.10"
    }
}
